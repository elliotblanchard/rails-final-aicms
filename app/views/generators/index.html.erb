<p class="title is-5">Generators</p>
<div class="column">
    <ol class="is-size-6">
    <% @generators.each do |generator| %>
        <li><%= link_to generator.name, generator_path(generator) %></li>
    <% end %>
    </ol>
</div>
<p class="subtitle is-6">
    <span class="tag is-primary"><%= link_to "Create new generator", new_generator_path %></span>
</p>